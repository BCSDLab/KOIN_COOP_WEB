{
  "search.exclude": {
    "**/yarn.lock": true,
    "**/README.md": true,
    "**/.eslintrc.json": true,
    "**/tsconfig.json": true,
    "**/.yarn": true,
    "**/.pnp.*": true,
    "**.svg": true
  },
  "eslint.nodePath": ".yarn/sdks",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  }
}
